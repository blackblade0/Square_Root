#include <stdio.h>
#include <math.h>

//Find the square of any number.

int main () {

double num;   
int i;

// Creating a loop that repeats three times.
for (i = 0;i <= 3; i++) {
   
    printf("Enter a number : ");
    scanf("%lf", &num);
    printf("%.4lf\n\n", sqrt(num)); //prints the squareroot to the 4th decimal places.
}

return 0;
}
